{
  "name": "project-name",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "github:soulfresh/project-name"
  },
  "engines": {
    "node": ">=11.10.1"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-classset": "^0.0.2",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-starter-kit": "^0.5.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.0",
    "@storybook/addon-links": "^5.2.0",
    "@storybook/addons": "^5.2.0",
    "@storybook/react": "^5.2.0",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-loader": "8.0.5",
    "babel-plugin-istanbul": "^5.2.0",
    "customize-cra": "^0.7.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-matchers": "^4.0.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "3.0.1",
    "rimraf": "^3.0.0",
    "source-map-explorer": "^2.1.0",
    "stylelint": "^10.1.0",
    "stylelint-a11y": "^1.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "analyze": "yarn build && source-map-explorer 'build/static/js/*.js' --html coverage/analysis/result.html",
    "clean": "rimraf ./{build,test-results,coverage}",
    "lint": "yarn eslint && yarn stylelint",
    "lint-fix": "yarn eslint-fix && yarn stylelint-fix",
    "stylelint": "stylelint 'src/**/*.scss'",
    "stylelint-fix": "yarn stylelint --fix",
    "eslint": "eslint 'src/**/*.js'",
    "eslint-fix": "yarn eslint --fix",
    "test": "karma start --reporters=progress,coverage --coverageType=summary",
    "test-debug": "karma start --log-level debug --jasmine-timeout=300000 --browsers=Firefox",
    "test-ci": "karma start --progress=false --singleRun=true --browsers=Chrome,Firefox --reporters=spec,junit,coverage --coverageType=ci",
    "coverage": "karma start --singleRun=true --reporters=dots,coverage --coverageType=ci",
    "validate": "yarn lint-fix && yarn test-ci && yarn analyze",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "deploy-storybook-ci": "storybook-to-ghpages -- --ci",
    "release": "./scripts/release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
